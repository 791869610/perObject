<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.family.mapper.FamilyAccountMapper" >
  <resultMap id="BaseResultMap" type="com.example.demo.family.model.FamilyAccount" >
    <!--
      WARNING - @mbg.generated
    -->
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="phone" property="phone" jdbcType="VARCHAR" />
    <result column="password" property="password" jdbcType="VARCHAR" />
    <result column="email" property="email" jdbcType="VARCHAR" />
    <result column="prison_partition_id" property="prisonPartitionId" jdbcType="BIGINT" />
    <result column="prisoner_name" property="prisonerName" jdbcType="VARCHAR" />
    <result column="prisoner_number" property="prisonerNumber" jdbcType="VARCHAR" />
    <result column="is_del" property="isDel" jdbcType="INTEGER" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
    <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
    <result column="reserved" property="reserved" jdbcType="VARCHAR" />
  </resultMap>

  <select id="selectFamilyAccountList" resultMap="BaseResultMap">
    SELECT
    id,
    phone,
    password,
    email,
    prison_partition_id,
    prisoner_name,
    prisoner_number,
    is_del,
    create_time,
    update_time,
    reserved
    FROM
    bus_family_account
    <where>
      is_del = 2
      <if test="id!=null">
        and id = #{id}
      </if>
    </where>
  </select>
</mapper>